+    {
+      List<Object> ar = new ArrayList<>();
+      int length = Array.getLength(array);
+      for (int i = 0; i < length; i++)
+      {
+        ar.add(Array.get(array, i));
+      }
+      return ar.toArray();
+    } else
+    {
+      return (Object[]) array;
+    }
+  }
